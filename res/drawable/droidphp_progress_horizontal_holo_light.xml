<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
            android:id="@android:id/background"
            android:drawable="@drawable/droidphp_progress_bg_holo_light"/>

    <item android:id="@android:id/secondaryProgress">
        <scale
                android:scaleWidth="100%"
                android:drawable="@drawable/droidphp_progress_secondary_holo_light"/>
    </item>

    <item android:id="@android:id/progress">
        <scale
                android:scaleWidth="100%"
                android:drawable="@drawable/droidphp_progress_primary_holo_light"/>
    </item>

</layer-list>
